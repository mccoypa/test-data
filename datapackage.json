{
  "name": "planetary-bodies",
  "title": "Planetary Bodies",		
  "description": "Test data for the github-viewer app. This is in DataPackage format (see http://www.dataprotocols.org/en/latest/data-packages.html).",
  "licenses": [
    {
      "id": "odc-odbl",
      "url": "http://opendatacommons.org/licenses/odbl/1.0/"
    }	
  ],
  "resources": [
    {
      "path": "data.csv",
      "schema": {
        "fields": [
          {
            "id": "planetary_body", 
            "label": "Planetary body", 
            "type": "text"
          }, 
          {
            "id": "aphelion",
            "label": "Aphelion", 
            "type": "number",
            "unit": {
              "type": "physical",
              "id": "km"
            }
          }, 
          {
            "id": "acceleration", 
            "label": "Acceleration due to gravity", 
            "type": "number",
            "unit": {
              "type": "physical",
              "id": "m/s^2"
            }
          }
        ]
      }
    }
  ]
}
